{
    "name": "superbrave/partnerize-bundle",
    "description": "A Symfony bundle to talk to the Partnerize API.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim van den Oever",
            "email": "tvdo@superbrave.nl"
        },
        {
            "name": "Joost van Veelen",
            "email": "jvv@superbrave.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/config": "^3.4||^4.0||^5.0",
        "symfony/dependency-injection": "^3.4||^4.0||^5.0",
        "symfony/http-kernel": "^3.4||^4.0||^5.0",
        "symfony/property-access": "^3.4||^4.0||^5.0",
        "symfony/property-info": "^3.4||^4.0||^5.0",
        "symfony/serializer": "^3.4||^4.0||^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpunit/php-code-coverage": "^8.0",
        "symfony/phpunit-bridge": "^3.4||^4.0||^5.0",
        "superbrave/coding-standards": "^0.2.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Superbrave\\PartnerizeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Superbrave\\PartnerizeBundle\\Tests\\": "tests/"
        }
    }
}
